  <!-- EN: recruit attempt -->
  <RecruitAttempt.label>koloniye katma girişimi</RecruitAttempt.label>
  <!-- EN:
    <li>r_logentry->[INITIATOR_nameDef] asked [RECIPIENT_nameDef] to join.</li>
    <li>r_logentry->[INITIATOR_nameDef] promised [goodthing] to [RECIPIENT_nameDef], and asked [RECIPIENT_objective] to join.</li>
    <li>goodthing(p=15)->[ConceptPositive]</li>
    <li>goodthing->free, fresh [Vegetable]s</li>
  -->
  <RecruitAttempt.logRulesInitiator.rulesStrings>
    <li>r_logentry->[INITIATOR_nameDef] [RECIPIENT_nameDef]'den koloniye katılmasını istedi.</li>
    <li>r_logentry->[INITIATOR_nameDef] [RECIPIENT_nameDef]'e [goodthing] sözü verdi ve ona katılıp katılmayacağını sordu.</li>
    <li>goodthing(p=15)->[ConceptPositive]</li>
    <li>goodthing->bedava, taze [Vegetable]ler</li>
  </RecruitAttempt.logRulesInitiator.rulesStrings>
  
  <!-- EN: spark jailbreak -->
