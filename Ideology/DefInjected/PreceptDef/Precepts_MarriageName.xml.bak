<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: always man's -->
  <MarriageName_AlwaysMans.label>her zaman erkeğin</MarriageName_AlwaysMans.label>
  <!-- EN: Couples must share the man's name. -->
  <MarriageName_AlwaysMans.description>Çiftler erkeğin soyadını paylaşmalıdır.</MarriageName_AlwaysMans.description>
  
  <!-- EN: always woman's -->
  <MarriageName_AlwaysWomans.label>her zaman ksoyadının</MarriageName_AlwaysWomans.label>
  <!-- EN: Couples must share the woman's name. -->
  <MarriageName_AlwaysWomans.description>Çiftler ksoyadının soyadını paylaşmalıdır.</MarriageName_AlwaysWomans.description>
  
  <!-- EN: keep names -->
  <MarriageName_KeepNames.label>isimleri sürdür</MarriageName_KeepNames.label>
  <!-- EN: Both partners keep their names upon marriage. -->
  <MarriageName_KeepNames.description>Her iki çift isimlerini evlilik sırasında sürdürür.</MarriageName_KeepNames.description>
  
  <!-- EN: random -->
  <MarriageName_Random.label>rastgele</MarriageName_Random.label>
  <!-- EN: Couples randomly choose whether to share a name, and which name to share. -->
  <MarriageName_Random.description>Çiftler, bir soyadı paylaşıp paylaşmayacaklarını ve hangi soyadı paylaşacaklarını rastgele seçerler.</MarriageName_Random.description>
  
  <!-- EN: usually woman's -->
  <MarriageName_UsuallyWomans.label>genellikle ksoyadının</MarriageName_UsuallyWomans.label>
  <!-- EN: Most couples share the woman's name. -->
  <MarriageName_UsuallyWomans.description>Çoğu çift ksoyadının soyadını paylaşır.</MarriageName_UsuallyWomans.description>
  
</LanguageData>